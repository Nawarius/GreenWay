{
    "parser":"babel-eslint",
    "env":{
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules":{
        "linebreak-style": "off",
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "no-trailing-spaces":"off",
        "eol-last":"off",
        "object-curly-spacing":"off",
        "require-jsdoc" : 0,
        "indent": ["error", 4]

    },
    "extends": ["eslint:recommended", "google"]
}